@model Entities.Trainer
@{
    ViewBag.Title = "Details";
}

<h2>Details Of Trainer Works </h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Subject</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        
            <tr>
                <td>@Model.TrainerId</td>
                <td>@Model.FirstName</td>
                <td>@Model.LastName</td>
                <td>@Model.Subject</td>
                <td>
                    <a href="/Trainer/Index" class="btn btn-info">Go Back</a>
                    <a href="#" class="btn btn-primary">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>

